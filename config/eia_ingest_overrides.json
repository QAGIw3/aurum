{
  "natural-gas/stor/wkly": {
    "schedule": "0 9 * * 5",
    "topic_var": "aurum_eia_ng_storage_topic",
    "default_topic": "aurum.ref.eia.ng.storage.v1",
    "series_id_expr": "series",
    "frequency": "WEEKLY",
    "units_var": "aurum_eia_ng_storage_units",
    "default_units": "BCF",
    "area_expr": "duoarea",
    "sector_expr": "process",
    "description_expr": "`series-description`",
    "source_expr": "COALESCE(source, 'EIA')",
    "dataset_expr": "COALESCE(dataset, 'nat_gas_storage')",
    "metadata_expr": "NULL",
    "filter_expr": "TRUE",
    "param_overrides": [
      {
        "data[0]": "value"
      }
    ],
    "source_name": "eia_ng_storage",
    "unit_conversion": {
      "source_unit": "BCF",
      "target_unit": "MMcf",
      "factor": 1000.0
    }
  },
  "electricity/rto/daily-region-data": {
    "schedule": "15 5 * * *",
    "topic_var": "aurum_eia_rto_region_topic",
