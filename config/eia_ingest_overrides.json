    "param_overrides": [
      {
        "data[0]": "value"
      }
    ],
    "source_name": "eia_rto_region_daily",
    "unit_conversion": {
      "source_unit": "MWh",
      "target_unit": "GWh",
      "factor": 0.001
    }
  },
