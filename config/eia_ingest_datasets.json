      "source_expr": "COALESCE(source, 'EIA')",
      "dataset_expr": "COALESCE(dataset, 'rto_daily_fuel')",
      "metadata_expr": "NULL",
      "filter_expr": "respondent IS NOT NULL",
      "param_overrides": [
        {
          "data[0]": "value"
        }
      ]
    },
    {
      "source_name": "eia_rto_interchange_daily",
      "path": "electricity/rto/daily-interchange-data",
      "data_path": "electricity/rto/daily-interchange-data/data",
      "description": "EIA RTO daily interchange by neighboring BA",
      "schedule": "25 5 * * *",
      "topic_var": "aurum_eia_rto_interchange_topic",
      "default_topic": "aurum.ref.eia.rto.interchange.daily.v1",
      "series_id_expr": "CONCAT(fromba, '_', toba)",
      "frequency": "DAILY",
      "units_var": "aurum_eia_rto_interchange_units",
