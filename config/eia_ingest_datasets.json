{
  "datasets": [
    {
      "source_name": "eia_ng_storage",
      "path": "natural-gas/stor/wkly",
      "data_path": "natural-gas/stor/wkly/data",
      "description": "EIA weekly underground natural gas storage",
      "schedule": "0 9 * * 5",
      "topic_var": "aurum_eia_ng_storage_topic",
      "default_topic": "aurum.ref.eia.ng.storage.v1",
      "series_id_expr": "series",
      "frequency": "WEEKLY",
      "units_var": "aurum_eia_ng_storage_units",
      "default_units": "BCF",
      "area_expr": "duoarea",
      "sector_expr": "process",
      "description_expr": "`series-description`",
      "source_expr": "COALESCE(source, 'EIA')",
      "dataset_expr": "COALESCE(dataset, 'nat_gas_storage')",
      "metadata_expr": "NULL",
      "filter_expr": "TRUE",
      "param_overrides": [
        {
          "data[0]": "value"
        }
      ]
    },
    {
      "source_name": "eia_rto_region_daily",
      "path": "electricity/rto/daily-region-data",
      "data_path": "electricity/rto/daily-region-data/data",
      "description": "EIA RTO daily demand, forecast, and interchange",
      "schedule": "15 5 * * *",
      "topic_var": "aurum_eia_rto_region_topic",
      "default_topic": "aurum.ref.eia.rto.region.daily.v1",
      "series_id_expr": "CONCAT(respondent, '_', type)",
      "frequency": "DAILY",
      "units_var": "aurum_eia_rto_region_units",
      "default_units": "MWh",
      "area_expr": "respondent",
      "sector_expr": "type",
      "description_expr": "`type-name`",
      "source_expr": "COALESCE(source, 'EIA')",
      "dataset_expr": "COALESCE(dataset, 'rto_daily_region')",
      "metadata_expr": "NULL",
      "filter_expr": "respondent IS NOT NULL",
      "param_overrides": [
        {
          "data[0]": "value"
        }
      ]
    },
    {
      "source_name": "eia_rto_fuel_daily",
