{
  "datasets": [
    {
      "source_name": "eia_bulk_eba",
      "url": "https://api.eia.gov/bulk/EBA.zip",
      "description": "EIA bulk archive for balancing authority interchange (EBA)",
      "schedule": "30 * * * *",
      "topic_var": "aurum_eia_bulk_eba_topic",
      "default_topic": "aurum.ref.eia.bulk.eba.v1",
      "frequency": "HOURLY",
      "series_id_expr": "series_id",
      "period_expr": "period",
      "value_expr": "value",
      "raw_value_expr": "value",
      "units_expr": "units",
      "area_expr": "area",
      "sector_expr": "sector",
      "description_expr": "description",
      "source_expr": "source",
      "dataset_expr": "dataset",
      "metadata_expr": "metadata",
      "filter_expr": "series_id IS NOT NULL",
      "skip_header": 1,
      "schema_fields": [
        {"name": "series_id", "type": "string"},
        {"name": "period", "type": "string"},
        {"name": "value", "type": "string"},
        {"name": "units", "type": "string"},
        {"name": "area", "type": "string"},
        {"name": "sector", "type": "string"},
        {"name": "description", "type": "string"},
        {"name": "source", "type": "string"},
        {"name": "dataset", "type": "string"},
        {"name": "metadata", "type": "string"}
      ],
      "extra_env": {
        "EIA_BULK_METADATA_EXPR": "metadata"
      }
    },
    {
      "source_name": "eia_bulk_coal",
      "url": "https://api.eia.gov/bulk/COAL.zip",
      "description": "EIA bulk archive for coal datasets",
      "schedule": "0 6 * * *",
      "topic_var": "aurum_eia_bulk_coal_topic",
      "default_topic": "aurum.ref.eia.bulk.coal.v1",
      "frequency": "MONTHLY",
      "series_id_expr": "series_id",
      "period_expr": "period",
      "value_expr": "value",
      "raw_value_expr": "value",
      "units_expr": "units",
      "area_expr": "area",
      "sector_expr": "sector",
      "description_expr": "description",
      "source_expr": "source",
      "dataset_expr": "dataset",
      "metadata_expr": "metadata",
      "filter_expr": "period IS NOT NULL",
      "csv_delimiter": ",",
      "skip_header": 1,
      "schema_fields": [
        {"name": "series_id", "type": "string"},
        {"name": "period", "type": "string"},
        {"name": "value", "type": "string"},
        {"name": "units", "type": "string"},
        {"name": "area", "type": "string"},
        {"name": "sector", "type": "string"},
        {"name": "description", "type": "string"},
        {"name": "source", "type": "string"},
        {"name": "dataset", "type": "string"},
        {"name": "metadata", "type": "string"}
      ]
    }
  ]
}
