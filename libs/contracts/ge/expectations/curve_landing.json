        "column": "price_type",
        "value_set": ["MID", "BID", "ASK", "LAST", "SETTLE", "INDEX", "STRIP", "CUSTOM"]
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "tenor_type",
        "value_set": ["MONTHLY", "QUARTER", "SEASON", "CALENDAR", "STRIP", "CUSTOM"]
      }
    }
  ]
